variable "aws_profile" {
  description = "Use this attribute to override the autogenerated profile name."
  default     = "vogt4nick-dev"
}

variable "aws_region" {
  description = "AWS region to run the Terraform script."
  default     = "us-east-1"
}

variable "common_tags" {
  type        = map(string)
  description = "Common tags that all resources should have."
  default     = { project = "vogt4nick.com" }
}
